<?xml version="1.0" encoding="utf-8"?>
<TfrxReport Version="3.24" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="За замовчуванням" ReportOptions.CreateDate="40162,6268214699" ReportOptions.Description.Text="" ReportOptions.LastChange="42921,9571920486" ScriptLanguage="PascalScript" ScriptText.Text="{****************************************************************&#13;&#10;**  Скрипт обсчёта тахеометрической съёмки 5&#34; прибором         **&#13;&#10;**               автор СИЛКОВ ЕВГЕНИЙ                          **&#13;&#10;*****************************************************************}&#13;&#10;{----------------------------------------------------------------|&#13;&#10;| Для работ необходим съёмка в слое &#34;обозначения&#34; две точки базы |&#13;&#10;| 159 линия и 10 линия на точки с этих баз                       |&#13;&#10;|                                                                |&#13;&#10;|----------------------------------------------------------------}&#13;&#10;&#13;&#10;Var&#13;&#10;&#13;&#10;dX, dY, PoX, WidthY, HeightX, MAS, stX, stY,&#13;&#10;stX2, stY2, stX3,stY3, stX4,stY4 : double; //координаты для расчёта масштаба&#13;&#10;&#13;&#10;ListUsers : string;  // список владельцев&#13;&#10;&#13;&#10;C, I, K, M, PlanScale, PlanScale2, PlanScale3, planscale4, Scal : integer; // счётчики, масштабы&#13;&#10;&#13;&#10;TempScale, stXtemp, stYtemp :integer; //доя процедуры определения положения подписей&#13;&#10;&#13;&#10;Xa, Xb, Xc, Ya, Yb, Yc,                   //координаты точек по отношению к вершине&#13;&#10;AB, BC,  Xab,  Xbc, Yab, Ybc,             // длины линий по отношению к вершине&#13;&#10;UgAB, UgBC, UgB,                          //углы в вершине&#13;&#10;FirstSTX, SecondSTX, FirstSTY, SecondSTY, // временные точки съёмки для работы&#13;&#10;CosBC, SinBC : Double;&#13;&#10;&#13;&#10;GGSX, GGSY :Array[0..60] of double;&#13;&#10;NameGGS :Array[0..60] of String;&#13;&#10;NOMGGS :Array[0..60] of Integer;    // координаты, имена, порядковые номера ГГСов&#13;&#10;&#13;&#10;disGGS1, disGGS2, GGS1, GGS2 :Array[0..60] of Double; //расчётные дальности до ГГСов&#13;&#10;&#13;&#10;Fstr, UgolB, UgolBC, DesOfSek, EdOfSek, RumbBC, RumbAB, UgolAB : String; // рабочие переменные&#13;&#10;&#13;&#10;Stanciya,Ugolbarr, CC, DXbcArr, DYbcArr, XcArr, YcArr,&#13;&#10;BCarr, RumbBCarr, Xbarr, Ybarr,  CosBCarr, SinBCarr, UgolABarr,&#13;&#10;UgolBCarr :array[0..60] of string;// списки для записи и воспроизведения значений&#13;&#10;&#13;&#10;//===================================================================================&#13;&#10;&#13;&#10;procedure Picture2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   with Picture2, Engine do&#13;&#10;   begin&#13;&#10;      VisibleGisMenu('Viewall',false);&#13;&#10;      VisibleGisLayer(0,False);   // участок&#13;&#10;      VisibleGisLayer(1, true);   // план&#13;&#10;      VisibleGisLayer(2, false);   // агрогруппы&#13;&#10;      VisibleGisLayer(3, false);   // таблица расчётов&#13;&#10;      VisibleGisLayer(4, true);   // обозначения&#13;&#10;      VisibleGisLayer(5, false);   // ситуация&#13;&#10;      VisibleGisLayer(6, false);   // чертёж&#13;&#10;      VisibleGisLayer(7, false);   // каталог координат&#13;&#10;      VisibleGisLayer(8, false);   // горизонтали&#13;&#10;      VisibleGisLayer(9, false);   // оформление&#13;&#10;      VisibleGisLayer(10, true);  // общий&#13;&#10;      VisibleGisLayer(11, false);  // сервитуты&#13;&#10;      VisibleGisLayer(12, false);  // ограничения&#13;&#10;      VisibleGisLayer(13, false);  // аренда&#13;&#10;      VisibleGisLayer(14, false);  // теодолитный ход&#13;&#10;      VisibleGisLayer(15, false);  // карта&#13;&#10;      VisibleGisLayer(16, false);  // проектные дороги&#13;&#10;      VisibleGisLayer(17, false);  // сторонние земли&#13;&#10;      VisibleGisLayer(18, false);  // секущая линия&#13;&#10;      GisShift(&#60;DM1.QbObject.&#34;XCenter&#34;&#62;,&#60;DM1.QbObject.&#34;YCenter&#34;&#62;);&#13;&#10;      DrawWmf(Width * 100 /3.77953, Height * 100 /3.77953,PlanScale);&#13;&#10;      Loadfromfile( StartPath +'\gis.wmf');&#13;&#10;   end;&#13;&#10;end;&#13;&#10;&#13;&#10;//========================================================================&#13;&#10;procedure Picture1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;      VisibleGisMenu('Viewall',false);&#13;&#10;      VisibleGisLayer(0, False);   // участок&#13;&#10;      VisibleGisLayer(1, True);   // план&#13;&#10;      VisibleGisLayer(2, false);   // агрогруппы&#13;&#10;      VisibleGisLayer(3, false);   // таблица расчётов&#13;&#10;      VisibleGisLayer(4, true);   // обозначения&#13;&#10;      VisibleGisLayer(5, False);   // ситуация&#13;&#10;      VisibleGisLayer(7, false);   // каталог координат&#13;&#10;      VisibleGisLayer(9, false);   // оформление&#13;&#10;      VisibleGisLayer(10, false);  // общий&#13;&#10;      VisibleGisLayer(11, true);  // сервитуты&#13;&#10;      VisibleGisLayer(12, False);  // ограничения&#13;&#10;      VisibleGisLayer(13, false);  // аренда&#13;&#10;      VisibleGisLayer(14, false);  // теодолитный ход&#13;&#10;      VisibleGisLayer(15, false);  // карта&#13;&#10;      VisibleGisLayer(16, false);  // проектные дороги&#13;&#10;      VisibleGisLayer(17, false);  // сторонние земли&#13;&#10;      VisibleGisLayer(18, false);  // секущая линия&#13;&#10;&#13;&#10;       with Picture1, Engine do&#13;&#10;       begin&#13;&#10;               DrawWmf(Width * 100 /3.77953, Height * 100 /3.77953, PlanScale2);&#13;&#10;               Loadfromfile( StartPath +'\gis.wmf');&#13;&#10;       end;&#13;&#10;       with Picture3, Engine do&#13;&#10;       begin&#13;&#10;               DrawWmf(Width * 100 /3.77953, Height * 100 /3.77953,planscale4);&#13;&#10;               Loadfromfile( StartPath +'\gis.wmf');&#13;&#10;       end;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;//===================================================================&#13;&#10;procedure Page1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;      VisibleGisLayer(0, true);   // участок&#13;&#10;      VisibleGisLayer(1, true);   // план&#13;&#10;      VisibleGisLayer(4, true);   // обозначения&#13;&#10;      VisibleGisLayer(9, False);   // оформление&#13;&#10;      ADOQuery1.Close;&#13;&#10;      ADOQuery2.Close;&#13;&#10;end;&#13;&#10;&#13;&#10;// нахождение масштаба по заданым размерам рамки картинки----------------&#13;&#10;Procedure FindScale;&#13;&#10;Begin&#13;&#10;  if (dX/dY) &#62;= (HeightX/WidthY) Then&#13;&#10;       begin&#13;&#10;       PoX:= (int(dX));&#13;&#10;       MAS:= HeightX;&#13;&#10;       end&#13;&#10;  else&#13;&#10;       Begin&#13;&#10;       PoX:= (int(dY));&#13;&#10;       MAS:= WidthY;&#13;&#10;       end;&#13;&#10;Scal:= 200;&#13;&#10;       IF Pox &#62;= MAS*2 Then Scal:=400;&#13;&#10;       IF Pox &#62;= MAS*4 Then Scal:=500;&#13;&#10;       IF Pox &#62;= MAS*5 Then Scal:=800;&#13;&#10;       IF Pox &#62;= MAS*8 Then Scal:=1000;&#13;&#10;       IF Pox &#62;= MAS*10 Then Scal:=2000;&#13;&#10;       IF Pox &#62;= MAS*20 Then Scal:=5000;&#13;&#10;       IF Pox &#62;= MAS*50 Then Scal:=10000;&#13;&#10;       IF Pox &#62;= MAS*100 Then Scal:=15000;&#13;&#10;       IF Pox &#62;= MAS*150 Then Scal:=20000;&#13;&#10;       IF Pox &#62;= MAS*200 Then Scal:=25000;&#13;&#10;       IF Pox &#62;= MAS*250 Then Scal:=30000;&#13;&#10;       IF Pox &#62;= MAS*300 Then Scal:=50000;&#13;&#10;End;&#13;&#10;&#13;&#10;//Проверка (снять коментирование внизу))----------------&#13;&#10;procedure Checking;&#13;&#10;Begin&#13;&#10;       stX := &#60;DM1.QbObject.&#34;XCenter&#34;&#62; -  &#60;ADOQuery2.&#34;X&#34;&#62;;&#13;&#10;       stX := stX/(PlanScale*0.001);&#13;&#10;       stX := stX * 3.77953;&#13;&#10;&#13;&#10;       stY := &#60;DM1.QbObject.&#34;YCenter&#34;&#62; -  &#60;ADOQuery2.&#34;Y&#34;&#62;;&#13;&#10;       stY := stY/(PlanScale*0.001);&#13;&#10;       stY := stY * 3.77953;&#13;&#10;       with TfrxMemoView.Create(nil) do&#13;&#10;       begin&#13;&#10;               Top := Picture1.Top + Picture1.Height/2 + stX;&#13;&#10;               Left := Picture1.Left + Picture1.width/2 - stY;&#13;&#10;               Width := 300;&#13;&#10;               Height := 15;&#13;&#10;               HAlign := haCenter;&#13;&#10;               VAlign := vaCenter;&#13;&#10;               Font.Size :=8;&#13;&#10;               Text := vartostr(Xb);&#13;&#10;               Parent := Page1;&#13;&#10;       end;&#13;&#10;end;&#13;&#10;&#13;&#10;// Определяет плоложение относительно картинки&#13;&#10;procedure PlaceOfItem;&#13;&#10;Begin&#13;&#10;       stX := (&#60;DM1.QbObject.&#34;XCenter&#34;&#62; - &#60;ADOQuery2.&#34;X&#34;&#62;) * 3.77953;&#13;&#10;       stX := stX/(TempScale*0.001);&#13;&#10;       If &#60;DM1.QbObject.&#34;XCenter&#34;&#62; &#60; &#60;ADOQuery2.&#34;X&#34;&#62;&#13;&#10;       Then stX := stX + stXtemp;&#13;&#10;&#13;&#10;       stY := (&#60;DM1.QbObject.&#34;YCenter&#34;&#62; - &#60;ADOQuery2.&#34;Y&#34;&#62;) * 3.77953;&#13;&#10;       stY := stY/(TempScale*0.001);&#13;&#10;       If &#60;DM1.QbObject.&#34;YCenter&#34;&#62; &#62; &#60;ADOQuery2.&#34;Y&#34;&#62;&#13;&#10;       Then sty := stY + stYtemp;&#13;&#10;End;&#13;&#10;&#13;&#10;&#13;&#10;//=================================================================&#13;&#10;begin//============================================================&#13;&#10;//=================================================================&#13;&#10;&#13;&#10;dX:= &#60;DM1.QbObject.&#34;XMax&#34;&#62;-&#60;DM1.QbObject.&#34;XMin&#34;&#62;;&#13;&#10;dY:= &#60;DM1.QbObject.&#34;YMax&#34;&#62;-&#60;DM1.QbObject.&#34;YMin&#34;&#62;;&#13;&#10;&#13;&#10;//picture1 схема сети  planScale2----------------------------------&#13;&#10;WidthY:=15;&#13;&#10;HeightX:= 20;&#13;&#10;FindScale;&#13;&#10;PlanScale2:= scal;&#13;&#10;&#13;&#10;// Picture3 креслення перенесення меж в натуру---------------------&#13;&#10;WidthY:=13;&#13;&#10;HeightX:= 13;&#13;&#10;FindScale;&#13;&#10;Planscale4:= scal;&#13;&#10;&#13;&#10;// Picture2 этот масштаб использует planScale----------------------&#13;&#10;WidthY:=6;&#13;&#10;HeightX:=6;&#13;&#10;FindScale;&#13;&#10;PlanScale:= Scal;&#13;&#10;&#13;&#10;//масштаб подложки с треугольниками -------------------------------&#13;&#10;PlanScale3:=70000;&#13;&#10;&#13;&#10;//рабочие точки GGS------------------------------------------------&#13;&#10;&#13;&#10;GGSX[1]:=5202863;&#13;&#10;GGSX[2]:=5196766;&#13;&#10;GGSX[3]:=5190505;&#13;&#10;GGSX[4]:=5201053;&#13;&#10;GGSX[5]:=5193661;&#13;&#10;GGSX[6]:=5195952;&#13;&#10;GGSX[7]:=5185464;&#13;&#10;GGSX[8]:=5177203;&#13;&#10;GGSX[9]:=5185406;&#13;&#10;GGSX[10]:=5196914;&#13;&#10;GGSX[11]:=5206507;&#13;&#10;GGSX[12]:=5204036;&#13;&#10;GGSX[13]:=5210526;&#13;&#10;GGSX[14]:=5206082;&#13;&#10;GGSX[15]:=5216376;&#13;&#10;GGSX[16]:=5177629;&#13;&#10;GGSX[17]:=5180115;&#13;&#10;GGSX[18]:=5169896;&#13;&#10;GGSX[19]:=5195956;&#13;&#10;GGSX[20]:=5189044;&#13;&#10;&#13;&#10;GGSY[1]:=5414237;&#13;&#10;GGSY[2]:=5406981;&#13;&#10;GGSY[3]:=5413592;&#13;&#10;GGSY[4]:=5399380;&#13;&#10;GGSY[5]:=5399252;&#13;&#10;GGSY[6]:=5391737;&#13;&#10;GGSY[7]:=5394902;&#13;&#10;GGSY[8]:=5381578;&#13;&#10;GGSY[9]:=5381777;&#13;&#10;GGSY[10]:=5384958;&#13;&#10;GGSY[11]:=5379020;&#13;&#10;GGSY[12]:=5389065;&#13;&#10;GGSY[13]:=5395983;&#13;&#10;GGSY[14]:=5399710;&#13;&#10;GGSY[15]:=5402579;&#13;&#10;GGSY[16]:=5402295;&#13;&#10;GGSY[17]:=5378211;&#13;&#10;GGSY[18]:=5372741;&#13;&#10;GGSY[19]:=5366353;&#13;&#10;GGSY[20]:=5372558;&#13;&#10;&#13;&#10;NameGGS[1]:='Карла Маркса';&#13;&#10;NameGGS[2]:='Радивонівка';&#13;&#10;NameGGS[3]:='Могила Зелена';&#13;&#10;NameGGS[4]:='Могила Орлова';&#13;&#10;NameGGS[5]:='Ольгино';&#13;&#10;NameGGS[6]:='Софіївка';&#13;&#10;NameGGS[7]:='Нововасилівка';&#13;&#10;NameGGS[8]:='Кільтичія';&#13;&#10;NameGGS[9]:='Трояни';&#13;&#10;NameGGS[10]:='Могила Остра';&#13;&#10;NameGGS[11]:='балка Чабанська';&#13;&#10;NameGGS[12]:='Дубовий';&#13;&#10;NameGGS[13]:='Ужвино';&#13;&#10;NameGGS[14]:='Миколаївка';&#13;&#10;NameGGS[15]:='Могила Свинарська';&#13;&#10;NameGGS[16]:='Макарти';&#13;&#10;NameGGS[17]:='Дмитрівка';&#13;&#10;NameGGS[18]:='Приморський';&#13;&#10;NameGGS[19]:='Єлізаветівка';&#13;&#10;NameGGS[20]:='Кримка';&#13;&#10;&#13;&#10;&#13;&#10;// Алгоритм отбора двух наиболее близких точек -------------------------&#13;&#10;I:=1;&#13;&#10;K:=1;&#13;&#10;M:=1;&#13;&#10;&#13;&#10;NomGGS[1]:=1;&#13;&#10;&#13;&#10;&#13;&#10;For K:= 1 to 20 do&#13;&#10;Begin&#13;&#10;       disGGS1[K] :=sqrt(Abs(GGSX[K]-&#60;DM1.QbObject.&#34;XCenter&#34;&#62;)*Abs(GGSX[K]-&#60;DM1.QbObject.&#34;XCenter&#34;&#62;) + Abs(GGSY[K]-&#60;DM1.QbObject.&#34;YCenter&#34;&#62;)*Abs(GGSY[K]-&#60;DM1.QbObject.&#34;YCenter&#34;&#62;));&#13;&#10;       disGGS2[K] :=sqrt(Abs(GGSX[K]-&#60;DM1.QbObject.&#34;XCenter&#34;&#62;)*Abs(GGSX[K]-&#60;DM1.QbObject.&#34;XCenter&#34;&#62;) + Abs(GGSY[K]-&#60;DM1.QbObject.&#34;YCenter&#34;&#62;)*Abs(GGSY[K]-&#60;DM1.QbObject.&#34;YCenter&#34;&#62;));&#13;&#10;End;&#13;&#10;GGS1[1]:=  disGGS1[1];&#13;&#10;GGS1[2]:=  disGGS1[1];&#13;&#10;NomGGS[2]:= 1;&#13;&#10;NomGGS[1]:= 1;&#13;&#10;&#13;&#10;// Собственно сам поиск ----------------------------------------&#13;&#10;While M &#60; 3  do&#13;&#10;Begin&#13;&#10;       K:=1;&#13;&#10;       While K &#60; 21 do&#13;&#10;       Begin&#13;&#10;               IF M=1 Then&#13;&#10;               Begin&#13;&#10;               If GGS1[1] &#62; disGGS1[K] Then&#13;&#10;                       Begin&#13;&#10;                               GGS1[1] := disGGS1[K];&#13;&#10;                               NomGGS[1]:= k;&#13;&#10;                       End;&#13;&#10;               End;&#13;&#10;               IF M=2 Then&#13;&#10;               Begin&#13;&#10;                       If NomGGS[1]= 1  Then&#13;&#10;                       If NomGGS[1]= K  Then&#13;&#10;                       Begin&#13;&#10;                               GGS1[2] := disGGS1[2];&#13;&#10;                               NomGGS[2]:= 2;&#13;&#10;                       end;&#13;&#10;                       If NomGGS[1]&#60;&#62; K Then&#13;&#10;                       Begin&#13;&#10;                               If GGS1[2] &#62; disGGS1[K] Then&#13;&#10;                               Begin&#13;&#10;                                       GGS1[2] := disGGS1[K];&#13;&#10;                                       NomGGS[2]:= K;&#13;&#10;                               End;&#13;&#10;                       End;&#13;&#10;               End;&#13;&#10;       Inc(K);&#13;&#10;       End;&#13;&#10;&#13;&#10;inc(M);&#13;&#10;End;&#13;&#10;&#13;&#10;// Обработка владельцев землеьного участка --------------------------------&#13;&#10;ListUsers:= '';&#13;&#10;DATAFIRST('DM1.TbUser');&#13;&#10;&#13;&#10;While Not DATAEOF('DM1.TbUser') Do&#13;&#10;Begin&#13;&#10;       if Pos(ListUsers,&#60;DM1.TbUser.&#34;NameUser&#34;&#62;) = 0 Then&#13;&#10;       Begin&#13;&#10;               If (&#60;DM1.TbUser.&#34;DateFirst&#34;&#62; &#60;= Date) Then&#13;&#10;               Begin&#13;&#10;                       if (&#60;DM1.TbUser.&#34;DateLast&#34;&#62; &#62;= Date) or (&#60;DM1.TbUser.&#34;DateLast&#34;&#62; = 0) Then&#13;&#10;                       Begin&#13;&#10;                               If Length(ListUsers) &#62; 0 Then&#13;&#10;                               ListUsers := ListUsers +', ';&#13;&#10;                               ListUsers := ListUsers +  &#60;DM1.TbUser.&#34;NameUser&#34;&#62;;&#13;&#10;                       End;&#13;&#10;               End;&#13;&#10;      End;&#13;&#10;DATANEXT('DM1.TbUser');&#13;&#10;End;&#13;&#10;&#13;&#10;// Отбор базовых точек съёмки------------------------------&#13;&#10;With ADOQuery1 Do&#13;&#10;Begin&#13;&#10;       SQL.Clear;&#13;&#10;       SQL.Add('Select P.* From NotePoint P, Note N Where P.ID_Note = N.ID_Note  And N.PenStyle = 159  And N.ID_Type = 3 And N.ID_Object = ' + IntToStr(&#60;DM1.QbObject.&#34;ID_Object&#34;&#62;) + ' Order by P.ID_NotePoint');&#13;&#10;       Open;&#13;&#10;End;&#13;&#10;&#13;&#10;// Отбор всех точек ---------------------------------------&#13;&#10;With ADOQuery2 Do&#13;&#10;Begin&#13;&#10;       SQL.Clear;&#13;&#10;       SQL.Add('Select P.* From Object O, NotePoint P, Note N Where  O.ID_Object = ' + IntToStr(&#60;DM1.QbObject.&#34;ID_Object&#34;&#62;) + ' And N.ID_Object = O.ID_Object AND P.ID_Note = N.ID_Note And N.PenStyle = 10 And N.ID_Type = 3 Order by P.ID_NotePoint');&#13;&#10;       Open;&#13;&#10;End;&#13;&#10;&#13;&#10;&#13;&#10;ADOQuery1.First;&#13;&#10;       FirstSTX := &#60;ADOQuery1.&#34;X&#34;&#62;;&#13;&#10;       FirstSTY:= &#60;ADOQuery1.&#34;Y&#34;&#62;;&#13;&#10;ADOQuery1.Next;&#13;&#10;       SecondSTX := &#60;ADOQuery1.&#34;X&#34;&#62;;&#13;&#10;       SecondSTY:= &#60;ADOQuery1.&#34;Y&#34;&#62;;&#13;&#10;&#13;&#10;Fstr:= VarToStr(SecondSTX);&#13;&#10;&#13;&#10;//некоторые системы имеют неправильные региональные параметры, и возвращают запятые в SQL ответе&#13;&#10;if POs(',',Fstr)&#62;0 Then   Fstr:= Copy(Fstr,0,Pos(',',Fstr)-1)+'.'+ Copy(Fstr,Pos(',',Fstr)+1,Length(Fstr));&#13;&#10;&#13;&#10;&#13;&#10;QUERY('Select P.* From Object O, Note N, NotePoint P Where O.ID_Object = ' + IntToStr(&#60;DM1.QbObject.&#34;ID_Object&#34;&#62;) + ' And N.ID_Object = O.ID_Object  And N.PenStyle = 159 And N.ID_Type = 3 AND P.ID_Note = N.ID_Note Order by P.ID_NotePoint');&#13;&#10;QUERYSQL('Select P.* From Object O, Note N, NotePoint P Where O.ID_Object = ' + IntToStr(&#60;DM1.QbObject.&#34;ID_Object&#34;&#62;) + ' And N.ID_Object = O.ID_Object  And N.PenStyle = 10  And N.ID_Type = 3 AND  P.X =  ' + Fstr + ' Order by P.ID_NotePoint');&#13;&#10;&#13;&#10;//Ст1 цикл по точкам GPS------------------------------------------------&#13;&#10;&#13;&#10;ADOQuery1.First;&#13;&#10;C:=1;&#13;&#10;&#13;&#10;While ADOQuery1.EOF = False do&#13;&#10;Begin&#13;&#10;&#13;&#10;       //расстояние от ГГС до точек GPS ---------------------------------&#13;&#10;&#13;&#10;       GGS1[c]:= sqrt(Abs(GGSX[NOMGGS[1]]-&#60;ADOQuery1.&#34;X&#34;&#62;)*Abs(GGSX[NOMGGS[1]]-&#60;ADOQuery1.&#34;X&#34;&#62;) + Abs(GGSY[NOMGGS[1]]-&#60;ADOQuery1.&#34;Y&#34;&#62;)*Abs(GGSY[NOMGGS[1]]-&#60;ADOQuery1.&#34;Y&#34;&#62;));&#13;&#10;       GGS2[c]:=  sqrt(Abs(GGSX[NOMGGS[2]]-&#60;ADOQuery1.&#34;X&#34;&#62;)*Abs(GGSX[NOMGGS[2]]-&#60;ADOQuery1.&#34;X&#34;&#62;) + Abs(GGSY[NOMGGS[2]]-&#60;ADOQuery1.&#34;Y&#34;&#62;)*Abs(GGSY[NOMGGS[2]]-&#60;ADOQuery1.&#34;Y&#34;&#62;));&#13;&#10;&#13;&#10;&#13;&#10;       //подстановка &#34;ст1&#34; &#34;ст2&#34; ----------------------------------------&#13;&#10;&#13;&#10;       stX  :=  (&#60;DM1.QbObject.&#34;XCenter&#34;&#62; - &#60;ADOQuery1.&#34;X&#34;&#62;) * 3.77953;&#13;&#10;       stX2 := stX /(PlanScale*0.001);&#13;&#10;       stX  := stX /(PlanScale2*0.001);&#13;&#10;       stX3 := stX /(PlanScale3*0.001);&#13;&#10;       stX4 := stX /(PlanScale4*0.001);&#13;&#10;&#13;&#10;       stY  := (&#60;DM1.QbObject.&#34;YCenter&#34;&#62; - &#60;ADOQuery1.&#34;Y&#34;&#62;)* 3.77953;&#13;&#10;       stY2 := stY/(PlanScale*0.001);&#13;&#10;       stY  := stY/(PlanScale2*0.001);&#13;&#10;       stY3 := stY/(PlanScale3*0.001);&#13;&#10;       stY4 := stY/(PlanScale4*0.001);&#13;&#10;&#13;&#10;       with TfrxMemoView.Create(nil) do&#13;&#10;       begin&#13;&#10;               Top := Picture3.Top + Picture3.Height/2 + stX4;&#13;&#10;               Left := Picture3.Left + Picture3.width/2 - stY4;&#13;&#10;               Width := 30;&#13;&#10;               Height := 15;&#13;&#10;               HAlign := haCenter; VAlign := vaCenter;&#13;&#10;               Font.Size :=10;&#13;&#10;               Text := 'ст'+varToSTR(C);&#13;&#10;               Parent := Page7;&#13;&#10;       end;&#13;&#10;&#13;&#10;       with TfrxMemoView.Create(nil) do&#13;&#10;       begin&#13;&#10;               Top := Picture1.Top + Picture1.Height/2 + stX;&#13;&#10;               Left := Picture1.Left + Picture1.width/2 - stY;&#13;&#10;               Width := 30;&#13;&#10;        